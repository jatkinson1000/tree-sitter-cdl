{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "cdl",
  "rules": {
    "dataset": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "netcdf"
        },
        {
          "type": "SYMBOL",
          "name": "dataset_id"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "dimensions_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "dimensions_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "dimensions:"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "dimension_declarations"
          }
        }
      ]
    },
    "dimension_declarations": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dimension"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "dimension"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "dimension": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "size",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\d+"
              },
              {
                "type": "PATTERN",
                "value": "unlimited|UNLIMITED"
              }
            ]
          }
        }
      ]
    },
    "dataset_id": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^{][^{]*"
      }
    },
    "identifier_old": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z_][a-zA-Z0-9_.+\\-@]*"
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[a-zA-Z_]"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\\"
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\d+"
                  }
                ]
              },
              {
                "type": "PATTERN",
                "value": "[\\u0080-\\uFFFF]"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[a-zA-Z0-9_.@+-]"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "\\"
                    },
                    {
                      "type": "PATTERN",
                      "value": "[ !\"#$%&'()*,:;<=>?\\[\\\\\\]^`{|}~]"
                    }
                  ]
                },
                {
                  "type": "PATTERN",
                  "value": "[\\u0080-\\uFFFF]"
                }
              ]
            }
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}