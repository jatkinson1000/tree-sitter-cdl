{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cdl",
      "camelcase": "CDL",
      "title": "Common Data Language",
      "scope": "source.cdl",
      "file-types": [
        "cdl"
      ],
      "injection-regex": "^cdl$",
      "class-name": "TreeSitterCdl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the Common Data Language for representing NetCDF data.",
    "authors": [
      {
        "name": "Jack Atkinson",
        "email": "jack.atkinson1000@gmail.com",
        "url": "https://github.com/jatkinson1000"
      }
    ],
    "links": {
      "repository": "https://github.com/jatkinson1000/tree-sitter-cdl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
